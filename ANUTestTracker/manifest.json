{
  "name": "AIM End Time Manager",
  "version": "1.10",
  "description": "Helps users keep track of when tests are ending through the Accessible Information Management website.",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webNavigation",
    "https://*.accessiblelearning.com/*/SVC-ATS-TaskDuJour.aspx*"
  ],
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://teton.accessiblelearning.com/*/SVC-ATS-TaskDuJour.aspx*"
      ],
      "js": ["content.js"],
      "run_at": "document_end",
      "match_about_blank": true
    }
  ],
  "commands": {
    "sort": {
      "suggested_key": {
        "default": "Alt+Shift+S"
      },
      "description": "Sort the testing table"
    },
    "delivery": {
      "suggested_key": {
        "default": "Alt+Shift+D"
      },
      "description": "Gets delivery methods for the day"
    }
  }
}
